{% extends 'base.html.twig' %}

{% block body %}
<h1>Check Result</h1>
    <h2>{{ checkResult['message'] }}</h2>
    {% for key, value in checkResult['result'] %}
        <h2>{{ key }}</h2>
        {% for result in value.advisories %}
            <p>{{ result.title }}</p>
        {% endfor %}
    {% endfor %}
{% endblock %}